{
  "name": "z-std-pack",
  "description": "Utility library for JavaScript promises",
  "keywords": [
    "promises",
    "util",
    "functional",
    "server",
    "client",
    "browser"
  ],
  "version": "0.2.2",
  "author": "Jakob Mattsson <jakob.mattsson@gmail.com> (jakobmattsson.se)",
  "license": "MIT",
  "main": "./lib/index.js",
  "repository": {
    "type": "git",
    "url": "http://github.com/jakobmattsson/z-std-pack"
  },
  "bugs": "http://github.com/jakobmattsson/z-std-pack/issues",
  "files": [
    "lib",
    "LICENSE"
  ],
  "engines": {
    "npm": ">= 1.3 < 2",
    "node": ">= 0.10 < 0.11"
  },
  "devDependencies": {
    "browserify": "3.30.2",
    "jscov": "^0.5.7",
    "mocha": "^1.12.1",
    "mocha-term-cov-reporter": "^0.2.0",
    "chai-as-promised": "^4.1.0",
    "mocha-as-promised": "^2.0.0",
    "coffee-script": "^1.7.1"
  },
  "dependencies": {
    "z-builtins": "^0.1.1",
    "z-underscore": "^0.1.1",
    "z-core": "^0.5.0"
  },
  "scripts": {
    "build": "rm -rf lib && coffee -co lib src",
    "make-dist": "npm test && mkdir -p dist && browserify lib/index.js > dist/latest.js",
    "coverage": "jscov --expand --conditionals src .cov && JSCOV=.cov mocha --reporter mocha-term-cov-reporter",
    "test": "mocha --grep \"$TESTS\"",
    "pretest": "npm run build",
    "prepublish": "npm run build"
  }
}
